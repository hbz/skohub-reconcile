				"analyzer": {
					"default": {
						"type" : "custom",
						"tokenizer" : "standard",
						"filter": ["lowercase"]
					},
					"autocomplete": {
						"tokenizer": "edge_ngram_tokenizer",
						"filter": ["lowercase"]
					},
					"id": {
						"tokenizer": "keyword",
						"filter": "lowercase"
					},
					"ascii": {
						"tokenizer": "standard",
						"filter": ["lowercase", "asciifolding"]
					}
				},
				"tokenizer": {
					"edge_ngram_tokenizer": {
						"type": "edge_ngram",
						"min_gram": "2",
						"max_gram": "40",
